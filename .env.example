# Environment Variables for go.travel
# Copy this file to .env and fill in your actual API keys

# Gemini AI API Key (Get from: https://makersuite.google.com/app/apikey)
GEMINI_API_KEY=REPLACE_WITH_YOUR_GEMINI_API_KEY

# Google API Key (Get from: https://console.cloud.google.com/)
GOOGLE_API_KEY=REPLACE_WITH_YOUR_GOOGLE_API_KEY

# OpenWeatherMap API Key (Get from: https://openweathermap.org/api)
OPENWEATHERMAP_API_KEY=REPLACE_WITH_YOUR_OPENWEATHER_API_KEY

# Flask Environment (development/production)
FLASK_ENV=development